# Don't forget, %l3 also can perform %l1 %l2 actions

User_Alias WPEL3 = %l3

WPEL3 ALL=(root) \
        /bin/chgrp, \
        /bin/chmod, \
        /bin/chown, \
        /bin/kill, \
        /usr/bin/edit, \
        /usr/bin/mailq, \
        /usr/bin/openssl, \
        /usr/bin/pkill, \
        /usr/bin/rsync, \
        /usr/bin/service, \
        /usr/bin/touch
